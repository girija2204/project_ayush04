<div class="profile">
	<%= render 'shared/left_section' %>
	<div class="right-section">
		<div class="all_tabs">
			<div class="details_section">
				<h4>All Posts</h4>
				<% @user.posts.each do |post| %>
					<h5><strong><%= link_to post.title, post %></strong></h5>
					<%= post.content %>
				<% end %>
			</div>
		</div>
	</div>
</div>