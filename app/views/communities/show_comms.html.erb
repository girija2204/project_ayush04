<div class="dashboard">
	<%= render 'shared/left_section' %>
	<div class="comm-section">
		<div>
			<% provide(:title, @title) %>
			<div>
				<h3><%= @title %></h3>
				<% @comms.each do |comm| %>
					<p><strong><%= link_to comm.c_name, disc_board_path(comm.id) %></strong></p>
				<% end %>
			</div>
		</div>
	</div>
</div>